<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration><!-- 결과와 파라미터만 매핑 하는것 -->
	<typeAliases>
		<typeAlias type="kr.ac.sungkyul.beautyline.vo.UserVo" alias="uservo" />
		<typeAlias type="kr.ac.sungkyul.beautyline.vo.ListVo" alias="listvo" />
		<typeAlias type="kr.ac.sungkyul.beautyline.vo.VisitVo"
